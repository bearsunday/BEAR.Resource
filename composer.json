{
    "name": "bear/resource",
    "description": "RESTful service layer framework",
    "keywords": [
        "REST",
        "HATEOAS",
        "service layer"
    ],
    "homepage": "https://github.com/koriym/BEAR.Resource",
    "version": "0.6.0",
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "facebook/xhprof",
                "version": "0.9.2",
                "dist": {
                    "url": "https://github.com/facebook/xhprof/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/facebook/xhprof.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php"                    : ">=5.4.0",
        "ray/aop"                : ">=1.0.0-beta2",
        "ray/di"                 : ">=1.0.0-beta3",
        "aura/di"                : "1.0.*",
        "aura/signal"            : "1.0.*",
		"aura/installer-default" : "1.0.*",
        "doctrine/common"        : "2.3.*-dev",
        "guzzle/guzzle"          : "3.0.*"
    },
    "require-dev": {
        "facebook/xhprof"        : "dev-master"
    },
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "BEAR\\Resource": "src/"
        }
    }
}
